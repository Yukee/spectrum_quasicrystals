Cell[TextData[{
 "It is also possible to derive recursion relations on ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    SubscriptBox["\[CapitalGamma]", "q"], "n"], TraditionalForm]]],
 " using the second-order perturbation theory on wavefunctions:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     SubscriptBox["\[CapitalGamma]", "q"], "n"], "=", 
    RowBox[{
     RowBox[{"2", 
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox["\[Lambda]", "2"], ")"}], "q"], 
      SuperscriptBox["\[Omega]", "2"], 
      SuperscriptBox["z", 
       RowBox[{"-", "\[Tau]"}]], 
      SuperscriptBox[
       SubscriptBox["\[CapitalGamma]", "q"], 
       RowBox[{"n", "-", "2"}]]}], "+", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"\[Lambda]", "'"}], ")"}], "q"], 
      SuperscriptBox["\[Omega]", "3"], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"z", "'"}], ")"}], 
       RowBox[{"-", "\[Tau]"}]], 
      SuperscriptBox[
       SubscriptBox["\[CapitalGamma]", "q"], 
       RowBox[{"n", "-", "3"}]]}]}]}], TraditionalForm]]],
 "\nwith \[Lambda] = ",
 Cell[BoxData[
  FormBox[
   FractionBox["1", 
    RowBox[{"1", "+", 
     RowBox[{
      SuperscriptBox["\[Rho]", "2"], "/", "2"}]}]], TraditionalForm]]],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Lambda]", "'"}], "=", 
    FractionBox["1", 
     RowBox[{"1", "+", 
      RowBox[{"2", 
       SuperscriptBox["\[Rho]", "2"]}]}]]}], TraditionalForm]]],
 ".\n\nUsing that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     SubscriptBox["\[CapitalGamma]", "q"], 
     RowBox[{"n", "+", "1"}]], "/", 
    RowBox[{
     SuperscriptBox[
      SubscriptBox["\[CapitalGamma]", "q"], "n"], "~", "1"}]}], 
   TraditionalForm]]],
 ", we arrive at an implicit equation on ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Tau]", "q"], ":"}], TraditionalForm]]]
}], "Text",
 CellChangeTimes->{{3.638793157921402*^9, 3.638793233760907*^9}, {
  3.638867467730092*^9, 3.6388674710259113`*^9}, {3.6388675179860086`*^9, 
  3.638867641799417*^9}, {3.6388677347913218`*^9, 3.638867756321891*^9}, {
  3.6388678316498957`*^9, 3.638867853912799*^9}, {3.638867951122116*^9, 
  3.638867961577031*^9}, {3.639137619826544*^9, 3.63913769305884*^9}},
 Background->RGBColor[0.94, 0.91, 0.88]],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"eq", "[", 
    RowBox[{"q_", ",", "\[Tau]q_", ",", "\[Rho]_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{
     FractionBox["2", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"2", "+", 
         SuperscriptBox["\[Rho]", "2"]}], ")"}], "q"]], 
     SuperscriptBox["\[Omega]", "2"], 
     SuperscriptBox[
      RowBox[{"(", 
       FractionBox["\[Rho]", "2"], ")"}], 
      RowBox[{"-", "\[Tau]q"}]]}], "+", 
    RowBox[{
     FractionBox["1", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"1", "+", 
         RowBox[{"2", 
          SuperscriptBox["\[Rho]", "2"]}]}], ")"}], "q"]], 
     SuperscriptBox["\[Omega]", "3"], 
     SuperscriptBox["\[Rho]", 
      RowBox[{
       RowBox[{"-", "2"}], "\[Tau]q"}]]}], "-", "1"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.639137703392116*^9, 3.639137736176573*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"implicitTau", "[", 
   RowBox[{"q_", ",", "\[Rho]_"}], "]"}], ":=", 
  RowBox[{"\[Tau]", "/.", 
   RowBox[{"FindRoot", "[", 
    RowBox[{
     RowBox[{"eq", "[", 
      RowBox[{"q", ",", "\[Tau]", ",", "\[Rho]"}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"\[Tau]", ",", "0."}], "}"}]}], "]"}]}]}]], "Input",
 CellChangeTimes->{{3.6388680736087217`*^9, 3.6388681099086533`*^9}}],

