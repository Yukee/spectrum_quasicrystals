 RowBox[{"{", 
  RowBox[{"{", 
   RowBox[{"x", "\[Rule]", "0.34164078649987384`"}], "}"}], "}"}]], "Output",
 CellChangeTimes->{3.6387849077291307`*^9, 3.638795698375495*^9, 
  3.638851416432148*^9, 3.63885536540606*^9, 3.639124111712367*^9, 
  3.6391247727106247`*^9}]
}, Open  ]],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "0"], "=", 
    RowBox[{
     RowBox[{"2", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "-", 
         SuperscriptBox["\[Omega]", "4"]}], ")"}], "/", "5"}]}], "=", 
     RowBox[{
      RowBox[{"2", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "-", "2", "+", 
          RowBox[{"3", "\[Omega]"}]}], ")"}], "/", "5"}]}], "=", 
      RowBox[{"2", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"3", "\[Omega]"}], "-", "1"}], ")"}], "/", "5"}]}]}]}]}], 
   TraditionalForm]]],
 "\nWe have ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", 
