Cell[BoxData[
 RowBox[{
  RowBox[{"linFit", "[", 
   RowBox[{"q_", ",", "\[Rho]_", ",", "nv_"}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"avIntN", ",", "data"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"avIntN", "=", 
      RowBox[{"ParallelMap", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"avqInt", "[", 
          RowBox[{"q", ",", "#", ",", "\[Rho]"}], "]"}], "&"}], ",", "nv"}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"data", "=", 
      RowBox[{"glue", "[", 
       RowBox[{
        RowBox[{"Log", "[", 
         RowBox[{"Fibonacci", "[", "nv", "]"}], "]"}], ",", 
        RowBox[{"Log", "[", "avIntN", "]"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"LinearModelFit", "[", 
      RowBox[{"data", ",", "n", ",", "n"}], "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
