  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"avIntN", ",", "data"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"avIntN", "=", 
