  "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"compute", " ", "variance"}], " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"x0", "=", 
    FormBox[
     RowBox[{"2", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"3", "\[Omega]"}], "-", "1"}], ")"}], "/", "5"}]}],
     TraditionalForm]}], ";", " ", 
   RowBox[{"(*", " ", 
    RowBox[{
    "this", " ", "is", " ", "the", " ", "value", " ", "of", " ", "x", " ", 
     "that", " ", "makes", " ", "f", " ", "stationnary"}], " ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{"\[Sigma]", "=", 
    RowBox[{
     RowBox[{"-", 
      RowBox[{
       RowBox[{"f", "''"}], "[", "x0", "]"}]}], "*", 
     RowBox[{"Log", "[", 
      RowBox[{"\[Omega]", "^", 
       RowBox[{"-", "1"}]}], "]"}]}]}], ";"}], "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{
   "asymptotic", " ", "probability", " ", "density", " ", "of", " ", "x"}], 
   " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"freq", "[", "x_", "]"}], ":=", 
