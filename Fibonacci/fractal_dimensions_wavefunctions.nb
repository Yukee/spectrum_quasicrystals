  RowBox[{
   RowBox[{"f", "[", "x_", "]"}], ":=", " ", 
   FractionBox[
    RowBox[{
     RowBox[{
      RowBox[{"-", "x"}], " ", 
      RowBox[{"Log", "[", 
       RowBox[{"3", 
        RowBox[{"x", "/", "2"}]}], "]"}]}], " ", "+", " ", 
     RowBox[{
      FormBox[
       FractionBox[
        RowBox[{"(", 
         RowBox[{"1", "+", "x"}], ")"}], "3"],
       TraditionalForm], 
      RowBox[{"Log", "[", 
       RowBox[{"1", "+", "x"}], "]"}]}], " ", "-", " ", 
     RowBox[{
      FormBox[
       FractionBox[
        RowBox[{"1", " ", "-", " ", 
         RowBox[{"2", "x"}]}], "3"],
       TraditionalForm], 
      RowBox[{"Log", "[", 
       RowBox[{"1", "-", 
        RowBox[{"2", "x"}]}], "]"}]}]}], 
