Cell[TextData[{
 "To a first approximation, ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[CapitalGamma]", "n"], TraditionalForm]]],
 " and one can identify sites having approximately the same x for systems of \
length n and n+1. Then, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Tau]", "num"], TraditionalForm]]],
 " is also approximately only function of x.\nThus, we now average the \
partition function on sites having the same x, before finding \[Tau]."
}], "Text",
 CellChangeTimes->{{3.633521106309523*^9, 3.633521246958589*^9}},
 Background->GrayLevel[0.85]],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"ClearAll", "[", 
    RowBox[{"q", ",", "tau"}], "]"}], ";"}], "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{
   "Define", " ", "a", " ", "mean", " ", "partition", " ", "fucntion", " ", 
    "averaged", " ", "on", " ", "all", " ", "sites", " ", "having", " ", 
    "the", " ", "same", " ", "given", " ", "x"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"gammean", "[", 
    RowBox[{"q_", ",", "tau_"}], "]"}], ":=", 
   RowBox[{
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"Length", "@", "posl"}], ")"}], 
     RowBox[{"-", "1"}]], 
    RowBox[{"Plus", "@@", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{"gam", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Transpose", "[", "meanI", "]"}], "[", 
           RowBox[{"[", 
            RowBox[{"posl", "[", 
             RowBox[{"[", "#", "]"}], "]"}], "]"}], "]"}], ",", "bl", ",", 
          "q", ",", "tau"}], "]"}], "&"}], "/@", 
       RowBox[{"Range", "[", 
        RowBox[{"Length", "[", "posl", "]"}], "]"}]}], ")"}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"gammeanN", "[", 
    RowBox[{"q_", ",", "tau_"}], "]"}], ":=", 
   RowBox[{
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"Length", "@", "poslN"}], ")"}], 
     RowBox[{"-", "1"}]], 
    RowBox[{"Plus", "@@", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{"gam", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Transpose", "[", "meanIN", "]"}], "[", 
           RowBox[{"[", 
            RowBox[{"poslN", "[", 
             RowBox[{"[", "#", "]"}], "]"}], "]"}], "]"}], ",", "blN", ",", 
          "q", ",", "tau"}], "]"}], "&"}], "/@", 
       RowBox[{"Range", "[", 
        RowBox[{"Length", "[", "poslN", "]"}], "]"}]}], ")"}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"q", "=", "2."}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Tau]x", "/.", 
   RowBox[{"FindRoot", "[", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"gammean", "[", 
        RowBox[{"q", ",", "\[Tau]x"}], "]"}], 
       RowBox[{"gammeanN", "[", 
        RowBox[{"q", ",", "\[Tau]x"}], "]"}]], "-", "1"}], ",", 
     RowBox[{"{", 
      RowBox[{"\[Tau]x", ",", "1."}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"%", "/", 
  RowBox[{"(", 
   RowBox[{"q", "-", "1"}], ")"}]}]}], "Input",
