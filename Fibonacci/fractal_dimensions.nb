Cell[TextData[{
 "Using the definition of \[Alpha]:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"1", "/", 
     SubscriptBox["F", "n"]}], "\[Tilde]", " ", 
    SuperscriptBox[
     RowBox[{"(", 
      SuperscriptBox[
       SubscriptBox["\[CapitalDelta]", "a"], "n"], ")"}], 
     RowBox[{"\[Alpha]", "(", "a", ")"}]]}], TraditionalForm]]],
 " as n\[Rule]\[Infinity],\nwe get\n\[Alpha](a)  = ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["lim", 
     RowBox[{"n", "\[Rule]", "\[Infinity]"}]], " ", 
    FractionBox[
     RowBox[{"log", "(", " ", 
      RowBox[{"1", "/", 
       SubscriptBox["F", "n"]}], " ", ")"}], 
     RowBox[{"log", "(", " ", 
      SuperscriptBox[
       SubscriptBox["\[CapitalDelta]", "a"], "n"], " ", ")"}]]}], 
   TraditionalForm]]]
}], "Text",
 CellChangeTimes->{{3.630836617101616*^9, 3.630836766703209*^9}},
