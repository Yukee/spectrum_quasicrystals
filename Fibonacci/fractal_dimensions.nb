Cell["Compute generalized dimensions by box-counting ", "Text",
 CellChangeTimes->{{3.630299750431608*^9, 3.6302997955022097`*^9}},
 Background->RGBColor[0.94, 0.91, 0.88]],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{"take", " ", "a", " ", "fractal", " ", "set", " ", 
     RowBox[{"(", 
      RowBox[{
      "represented", " ", "as", " ", "an", " ", "ordered", " ", "list", " ", 
       "of", " ", "points", " ", "between", " ", "0", " ", "and", " ", "1", 
       " ", "each", " ", "associated", " ", "to", " ", "a", " ", "weight"}], 
      ")"}]}], ",", " ", 
    RowBox[{
     RowBox[{"return", " ", "qnorm"}], " ", "=", " ", 
     RowBox[{
      SubscriptBox["\[CapitalSigma]", "i"], " ", 
      SuperscriptBox[
       SubscriptBox["p", "i"], "q"]}]}], ",", " ", 
    RowBox[{"where", " ", 
     SubscriptBox["p", "i"], " ", "is", " ", "the", " ", "weight", " ", "of", 
     " ", "the", " ", 
     SuperscriptBox["i", "th"], " ", 
     RowBox[{"box", ".", " ", "Explicitely"}]}], ",", " ", 
    RowBox[{
     RowBox[{
      SubscriptBox["p", "i"], " ", "is", " ", "the", " ", "sum", " ", "of", 
      " ", "the", " ", "weights", " ", "of", " ", "the", " ", "pts", " ", 
      "in", " ", "the", " ", 
      SuperscriptBox["i", "th"], " ", 
      RowBox[{"box", ":", " ", 
       SubscriptBox["p", "i"]}]}], " ", "=", " ", 
     RowBox[{
      SubscriptBox["\[CapitalSigma]", 
       RowBox[{"s", "\[Element]", 
        SubscriptBox["b", "i"]}]], 
      RowBox[{
       SubscriptBox["w", "s"], "."}]}]}]}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"Clear", "[", "WeightedBc", "]"}], ";"}], "\n", 
   RowBox[{
    RowBox[{"WeightedBc", "[", 
     RowBox[{
     "pts_", ",", "weights_", ",", "n_", ",", "\[Delta]_", ",", "q_"}], "]"}],
     ":=", 
    RowBox[{"Block", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"curLbl", "=", "1"}], ",", 
        RowBox[{"curBS", "=", "0"}], ",", "\[CapitalDelta]", ",", 
        RowBox[{"b", "=", 
         RowBox[{"\[Delta]", "^", 
          RowBox[{"-", "n"}]}]}], ",", 
        RowBox[{"qnorm", "=", "0"}], ",", 
        RowBox[{"ntot", "=", 
         RowBox[{"Length", "[", "pts", "]"}]}], ",", 
        RowBox[{"curweight", "=", "0"}], ",", 
        RowBox[{"len", "=", 
         RowBox[{"Length", "[", "pts", "]"}]}], ",", "loop", ",", "p0"}], 
       "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"p0", "=", 
        RowBox[{"pts", "[", 
         RowBox[{"[", "curLbl", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{"\[CapitalDelta]", ":", 
         RowBox[{
         "distance", " ", "btw", " ", "the", " ", "left", " ", "side", " ", 
          "of", " ", "the", " ", "current", " ", "box", " ", "and", " ", 
          "the", " ", "current", " ", "pt"}]}], "*)"}], "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"b", "\[GreaterEqual]", "p0"}], ",", 
           RowBox[{"\[CapitalDelta]", "=", 
            RowBox[{"b", "-", "p0"}]}], ",", 
           RowBox[{"\[CapitalDelta]", "=", 
            RowBox[{"Mod", "[", 
             RowBox[{"p0", ",", "b"}], "]"}]}]}], "]"}], ";"}], "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{"\[CapitalDelta]", "=", 
        RowBox[{"Max", "[", 
         RowBox[{
          RowBox[{"b", "-", "p0"}], ",", "0"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{
        "position", " ", "of", " ", "the", " ", "current", " ", "box", " ", 
         "left", " ", "side"}], " ", "*)"}], "\[IndentingNewLine]", 
       RowBox[{"curBS", "=", 
        RowBox[{"p0", "-", "\[CapitalDelta]"}]}], ";", "\[IndentingNewLine]", 
       
       RowBox[{"While", "[", 
        RowBox[{
         RowBox[{"curLbl", "\[LessEqual]", "len"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"(*", " ", 
          RowBox[{
          "there", " ", "is", " ", "for", " ", "now", " ", "0", " ", "pt", 
           " ", "in", " ", "the", " ", "current", " ", "box"}], " ", "*)"}], 
         "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"curweight", "=", "0"}], ";", "\[IndentingNewLine]", 
          RowBox[{"(*", 
           RowBox[{
            RowBox[{
            "while", " ", "the", " ", "current", " ", "pt", " ", "is", " ", 
             "in", " ", "the", " ", "current", " ", "box"}], ",", " ", 
            RowBox[{
            "add", " ", "its", " ", "weight", " ", "and", " ", "pass", " ", 
             "to", " ", "the", " ", "pt", " ", "on", " ", "its", " ", 
             "right"}]}], "*)"}], "\[IndentingNewLine]", 
          RowBox[{"While", "[", 
           RowBox[{
            RowBox[{"\[CapitalDelta]", "\[LessEqual]", "b"}], ",", 
            "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"curweight", "+=", 
              RowBox[{"weights", "[", 
               RowBox[{"[", "curLbl", "]"}], "]"}]}], ";", 
             "\[IndentingNewLine]", 
             RowBox[{"curLbl", "++"}], ";", "\[IndentingNewLine]", 
             RowBox[{"If", "[", 
              RowBox[{
               RowBox[{"curLbl", "\[LessEqual]", " ", "len"}], ",", 
               RowBox[{"\[CapitalDelta]", "=", 
                RowBox[{
                 RowBox[{"pts", "[", 
                  RowBox[{"[", "curLbl", "]"}], "]"}], "-", "curBS"}]}], ",", 
               
               RowBox[{"Break", "[", "]"}]}], "]"}], ";"}]}], 
           "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
          RowBox[{"(*", " ", 
           SuperscriptBox[
            SubscriptBox["p", "i"], "q"], " ", "*)"}], "\[IndentingNewLine]", 
          
          RowBox[{"qnorm", "+=", 
           RowBox[{
            RowBox[{"(", "curweight", ")"}], "^", "q"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"curLbl", ">", "len"}], ",", 
            RowBox[{"Break", "[", "]"}]}], "]"}], ";", "\[IndentingNewLine]", 
          
          RowBox[{"(*", 
           RowBox[{
           "after", " ", "the", " ", "loop", " ", "the", " ", "current", " ", 
            "box", " ", "has", " ", "jumped", " ", "of", " ", 
            RowBox[{"Floor", "[", 
             RowBox[{"\[CapitalDelta]", "/", "b"}], "]"}], " ", "units"}], 
           "*)"}], "\[IndentingNewLine]", 
          RowBox[{"curBS", "+=", 
           RowBox[{"b", " ", 
            RowBox[{"Floor", "[", 
             RowBox[{"\[CapitalDelta]", "/", "b"}], "]"}]}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"\[CapitalDelta]", "=", 
           RowBox[{
            RowBox[{"pts", "[", 
             RowBox[{"[", "curLbl", "]"}], "]"}], "-", "curBS"}]}], ";"}]}], 
        "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
       RowBox[{"Return", "[", "qnorm", "]"}], ";"}]}], "]"}]}]}]}]], "Input",
