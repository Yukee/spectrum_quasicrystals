Cell["Integrated local density of states", "Subsection",
 CellChangeTimes->{{3.633166761810474*^9, 3.6331667661702414`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"chemical", " ", "potential"}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"mu", "=", "1.99"}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "find", " ", "the", " ", "index", " ", "of", " ", "the", " ", "lowest", 
     " ", "energy", " ", "level", " ", "larger", " ", "than", " ", "the", " ",
      "chemical", " ", "potential"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"FirstPosition", "[", 
      RowBox[{"en", ",", 
       RowBox[{"e_", " ", "/;", " ", 
        RowBox[{"e", ">", "mu"}]}]}], "]"}], "//", "First"}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "index", " ", "of", " ", "the", " ", "highest", " ", "energy", " ", 
     "level", " ", "smaller", " ", "than", " ", "the", " ", "chemical", " ", 
     "potential"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"%", "\[Equal]", 
       RowBox[{"Missing", "[", "\"\<NotFound\>\"", "]"}]}], ",", 
      RowBox[{"am", "=", "n"}], ",", ",", 
      RowBox[{"am", "=", 
       RowBox[{"%", "-", "1"}]}]}], "]"}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"index", " ", "wavefunctions", " ", "by", " ", "energies"}], ",",
      " ", 
     RowBox[{
     "keep", " ", "components", " ", "up", " ", "to", " ", "the", " ", 
      "Fermi", " ", "level"}], ",", " ", 
     RowBox[{"take", " ", "their", " ", "square"}]}], " ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"int", "=", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"eigs", "[", 
        RowBox[{"[", 
         RowBox[{";;", "am"}], "]"}], "]"}], ")"}], "2"]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"intRef", "=", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"eigsRef", "[", 
        RowBox[{"[", 
         RowBox[{";;", "am"}], "]"}], "]"}], ")"}], "2"]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "difference", " ", "in", " ", "integrated", " ", "local", " ", "density", 
     " ", "of", " ", "states"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"dPhi", "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"Plus", "@@", "int"}], ")"}], "-", 
      RowBox[{"(", 
       RowBox[{"Plus", "@@", "intRef"}], ")"}]}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"wavelength", " ", "at", " ", "the", " ", "Fermi", " ", "level"}],
     " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"\[Lambda]f", "=", 
     FractionBox[
      RowBox[{"2", "Pi"}], 
      RowBox[{"ArcCos", "[", 
       RowBox[{"mu", "/", "2"}], "]"}]]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"ListPlot", "[", 
    RowBox[{"dPhi", ",", 
     RowBox[{"PlotRange", "\[Rule]", "All"}], ",", 
     RowBox[{"Joined", "\[Rule]", "True"}], ",", 
     RowBox[{"PlotLabel", "\[Rule]", 
      RowBox[{"\"\<\!\(\*SubscriptBox[\(\[Lambda]\), \(F\)]\) = \>\"", "<>", 
       RowBox[{"ToString", "[", "\[Lambda]f", "]"}]}]}]}], "]"}]}]}]], "Input",\

