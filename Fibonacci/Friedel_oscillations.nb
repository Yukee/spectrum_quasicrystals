Cell[CellGroupData[{

Cell["Fibonacci in the direct basis", "Section",
 CellChangeTimes->{{3.633159117119451*^9, 3.633159130599331*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"\[Tau]", "=", 
   RowBox[{"N", " ", "[", 
    FractionBox[
     RowBox[{"1", "+", 
      RowBox[{"Sqrt", "[", "5", "]"}]}], "2"], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"rot", "=", "28"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"fib", "[", "n_", "]"}], ":=", 
   RowBox[{
    RowBox[{"IntegerPart", "[", 
     RowBox[{
      SuperscriptBox["\[Tau]", 
       RowBox[{"-", "1"}]], 
      RowBox[{"(", " ", 
       RowBox[{"n", "+", "1", "+", "rot"}], ")"}]}], "]"}], "-", 
    RowBox[{"IntegerPart", "[", 
     RowBox[{
      SuperscriptBox["\[Tau]", 
       RowBox[{"-", "1"}]], " ", 
      RowBox[{"(", 
       RowBox[{"n", "+", "rot"}], ")"}]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"jump", "[", 
    RowBox[{"n_", ",", "tw_"}], "]"}], ":=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"fib", "[", 
