Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "calcule", " ", "les", " ", "probabilit\[EAcute]s", " ", "de", " ", 
    "retour", " ", "au", " ", "site", " ", "en", " ", "milieu", " ", "de", 
    " ", "cha\[IHat]ne"}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"{", 
     RowBox[{"rmiddle", ",", "int"}], "}"}], "=", 
    RowBox[{"Block", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"n", "=", "10"}], ",", 
        RowBox[{"v", "=", ".3"}], ",", 
        RowBox[{"r", "=", ".3"}], ",", "val", ",", "vec", ",", "K", ",", 
        RowBox[{"tmax", "=", "1500"}], ",", 
        RowBox[{"dt", "=", "5."}], ",", "timelist", ",", "rmiddle", ",", 
        "middle", ",", "d", ",", "dtot", ",", "int"}], "}"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
       "on", " ", "prend", " ", "des", " ", "distances", " ", "entre", " ", 
        "sites", " ", "ppv", " ", "proportionnelles", " ", "aux", " ", 
        "amplitudes", " ", "de", " ", "saut"}], " ", "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"on", " ", "place", " ", 
        RowBox[{"l", "'"}], "origine", " ", "au", " ", "bord", " ", "gauche", 
        " ", "de", " ", "la", " ", "cha\[IHat]ne"}], " ", "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"dtot", "=", 
        RowBox[{"Sum", "[", 
         RowBox[{
          SuperscriptBox[
           RowBox[{"A", "[", 
            RowBox[{"i", ",", "v", ",", "r"}], "]"}], 
           RowBox[{"-", "1"}]], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", 
            SuperscriptBox["2", "n"]}], "}"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"d", "=", 
        RowBox[{
         SuperscriptBox["dtot", 
          RowBox[{"-", "1"}]], 
         RowBox[{"ParallelTable", "[", 
          RowBox[{
           RowBox[{"Sum", "[", 
            RowBox[{
             SuperscriptBox[
              RowBox[{"A", "[", 
               RowBox[{"i", ",", "v", ",", "r"}], "]"}], 
              RowBox[{"-", "1"}]], ",", 
             RowBox[{"{", 
              RowBox[{"i", ",", "j"}], "}"}]}], "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"j", ",", 
