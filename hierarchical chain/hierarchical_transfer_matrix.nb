  RowBox[{
   RowBox[{"dWF", "[", 
    RowBox[{
     RowBox[{"q_", "?", "NumberQ"}], ",", "i_"}], "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"v", "=", "1."}], ",", 
       RowBox[{"r", "=", ".1"}], ",", "chi", ",", "chiN", ",", "vec", ",", 
