Cell[TextData[{
 "We want to check the conjecture\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     SubscriptBox["D", "q"], "\[Mu]"], "=", 
    RowBox[{
     SuperscriptBox[
      SubscriptBox["D", "q"], "\[Psi]"], 
     SubscriptBox["D", 
      RowBox[{"1", "+", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"q", "-", "1"}], ")"}], 
        SuperscriptBox[
         SubscriptBox["D", "q"], "\[Psi]"]}]}]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nIt remains to compute ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["D", 
    RowBox[{"q", "'"}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " for q\[CloseCurlyQuote] in the range ",
 Cell[BoxData[
  FormBox[
   RowBox[{"1", "+", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"q", "-", "1"}], ")"}], 
     SuperscriptBox[
      SubscriptBox["D", "q"], "\[Psi]"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
